ALTER TABLE ONLY Countries
    ADD CONSTRAINT Countries_pkey PRIMARY KEY (code);


CREATE INDEX fki_dab ON Indicators USING btree (countrycode);


ALTER TABLE ONLY Indicators
    ADD CONSTRAINT fkcountryCode FOREIGN KEY (countrycode) REFERENCES Countries(code);


